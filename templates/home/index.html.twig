{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!
{% endblock %}

{% block body %}
	{% for post in posts %}
		<h2>{{ post.title }}</h2>
		<p>{{ post.description }}</p>
		<p>{{ post.user.name }} </p>
		{% for tag in post.tags %}
			<i>{{ tag.name }}</i>
		{% endfor %}
		<hr>
	{% endfor %}
{% endblock %}
